{
  "name": "vigour-doc-badges",
  "version": "1.1.1",
  "description": "Generates `.md` badges",
  "main": "lib/",
  "scripts": {
    "test": "node test/index.js | tap-spec",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "view-cover": "istanbul report html && open ./coverage/index.html",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/doc-badges.git"
  },
  "keywords": [
    "vigour",
    "doc",
    "docs",
    "vigour-doc",
    "badges",
    "travis",
    "standard",
    "npm"
  ],
  "author": "Vigour <dev@vigour.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/doc-badges/issues"
  },
  "homepage": "https://github.com/vigour-io/doc-badges#readme",
  "docs": "https://github.com/vigour-io/doc-badges/tree/master/docs",
  "contributors": [
    "Shawn Inder <shawn@vigour.io>"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "lodash.template": "^4.2.2"
  },
  "engines": {},
  "devDependencies": {
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  }
}
